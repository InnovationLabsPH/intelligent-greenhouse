{
  "version": 2,
  "name": "intelligent-greenhouse",
  "builds": [
    {
      "src": "Web-Dashboard/front-end/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd Web-Dashboard/front-end && npm ci && npm run build",
        "outputDirectory": "Web-Dashboard/front-end/build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
